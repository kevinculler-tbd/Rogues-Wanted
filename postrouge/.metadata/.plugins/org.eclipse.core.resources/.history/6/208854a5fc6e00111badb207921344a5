package main.java.Model.Items;

import main.java.actions.AbstractAction;
import main.java.actors.DisplayObject;
import main.java.actors.ActorObjects.AbstractActorObject;
import main.java.util.PlayerSessionUtil.ItemType;

public class Jacket extends AbstractItem{

	@Override
	public AbstractAction use(AbstractActorObject user) {
		
		description = "A Jacket";
		icon = new DisplayObject(0x000000, 0x0000ee, "j");
		type = ItemType.SELF;
		return null;
	}

}
