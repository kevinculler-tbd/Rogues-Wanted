package main.java.Model;

public class Player {

	private int x;
	private int y;
	private int hp;
	private char token; 
	
	public Player(int xPos, int yPos, int initHp, char initToken){
		x=xPos;
		y=yPos;
		hp= initHp;
		token = initToken;
	}

	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getHp() {
		return hp;
	}
	public void setHp(int hp) {
		this.hp = hp;
	}
	
	public char getToken() {
		return token;
	}

	public void setToken(char token) {
		this.token = token;
	}
	
}
